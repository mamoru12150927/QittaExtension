{
    "name": "Qitta Show read time counter",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Build an Extension!",
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "test"
    },
    "permissions": [
      "tabs",
      "background",
      "http://*/*",
      "https://*/*"
    ]
}